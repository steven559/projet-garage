{% extends 'base.html.twig' %}

{% block title %}UserId index{% endblock %}

{% block body %}
    <h1>UserId index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Jour</th>
                <th>HeurDepart</th>
                <th>HeurFin</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for user_id in user_ids %}
            <tr>
                <td>{{ user_id.id }}</td>
                <td>{{ user_id.jour }}</td>
                <td>{{ user_id.heurDepart }}</td>
                <td>{{ user_id.heurFin }}</td>
                <td>
                    <a href="{{ path('user_id_show', {'id': user_id.id}) }}">show</a>
                    <a href="{{ path('user_id_edit', {'id': user_id.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('user_id_new') }}">Create new</a>
{% endblock %}
